<%- include('./partials/_header'); %>
  <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
    <div
      class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
      <h4>Listado</h4>
    </div>


    <div class="table-responsive">
      <table id="example" class="table table-striped table-sm table-hover datatables">
          <thead>
              <tr>
                  <th>ID_ESTAB</th>
                  <th>COD_DEIS</th>
                  <th>NOMB_DEIS</th>
                  <th>TIPO_ESTAB</th>
                  <th>DEPENDENCIA</th>
                  <th>VIGENTE</th>
                  <th>ID_COMUNA</th>
                  <th>COD_LE</th>
              </tr>
          </thead>
      <tbody>
          <% if(data) { %>
              <% for(var i=0; i < data.length; i++) { %>
                  <tr>
                    <td><%= data[i].ID_ESTAB %></td>
                    <td><%= data[i].COD_DEIS %></td>
                    <td><%= data[i].NOMB_DEIS %></td>
                    <td><%= data[i].TIPO_ESTAB %></td>
                    <td><%= data[i].DEPENDENCIA %></td>
                    <td><%= data[i].VIGENTE %></td>
                    <td><%= data[i].ID_COMUNA %></td>
                    <td><%= data[i].COD_LE %></td>
                  </tr>
                  <% } %>
          <%}%>
      </tbody>
    </table>
  </div>
  </main>



    
  <%- include('./partials/_footer'); %>